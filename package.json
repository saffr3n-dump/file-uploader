{
  "name": "file-uploader",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm i && prisma generate",
    "start": "node --env-file=.env src/index.js",
    "dev": "node --env-file=.env --watch src/index.js",
    "lint": "eslint .",
    "format": "prettier -w . && prisma format",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0"
  }
}
